{
    "version": 2,
    "builds": [
        {
            "src": "./index.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
        {
          "source": "(*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        } 
    ]
}